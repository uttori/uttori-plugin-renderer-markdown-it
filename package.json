{
  "name": "uttori-renderer-markdown-it",
  "version": "1.2.0",
  "description": "Uttori renderer support for Markdown powered by MarkdownIt.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-renderer-markdown-it",
