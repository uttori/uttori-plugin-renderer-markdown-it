{
  "name": "@uttori/plugin-renderer-markdown-it",
  "version": "4.5.0",
  "description": "Uttori plugin for rendering Markdown powered by MarkdownIt.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": "uttori/uttori-plugin-renderer-markdown-it",
  "homepage": "https://github.com/uttori/uttori-plugin-renderer-markdown-it",
  "keywords": [
    "uttori",
    "renderer",
    "markdown",
    "markdownit",
    "markdown-it-plugin"
  ],
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "markdown-it": "^13.0.1",
    "slugify": "^1.6.5"
  },
  "optionalDependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@typescript-eslint/parser": "^5.42.0",
    "ava": "^5.0.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-anti-trojan-source": "^1.1.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-xss": "^0.1.12",
    "jsdoc": "^4.0.0",
    "jsdoc-to-markdown": "^7.1.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4"
  },
  "files": [
    "esm/*",
    "src/*",
    "types/*"
  ],
  "main": "src/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./src/index.js",
      "import": "./esm/index.js"
    },
    "./markdown-it-plugin": {
      "types": "./types/index.d.ts",
      "require": "./src/markdown-it-plugin.js"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "!src/**/*.js"
    ],
    "concurrency": 1,
    "failFast": false,
    "tap": false,
    "verbose": true
  },
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js --template rm.hbs --files src/index.js > README.md; jsdoc2md --private --example-lang js --files src/index.js > docs/render-markdownit.md; jsdoc2md --private --example-lang js --files src/markdown-it-plugin.js > docs/markdown-it-plugin.md",
    "lint": "eslint .",
    "make-types": "rm -fr types/ && mkdir types && tsc --project .tsconfig.json",
    "release": "release-it",
    "report": "nyc report --reporter=html",
    "test": "NODE_ENV=test nyc ava",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test ava --serial --concurrency=1",
    "validate": "npm ls"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
